{
  "name": "Emoji Widget",
  "description": "Widget for picking Emojis",
  "version": "3.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "identity",
    "identity.email"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the widget"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+X",
        "windows": "Ctrl+Shift+X",
        "mac": "Command+Shift+X",
        "linux": "Ctrl+Shift+X"
      }
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "192": "favicon-192x192.png"
  },
  "oauth2": {
    "client_id": "183747301370-9p4aiv6c53r3pktl6phujp7gch0593i2.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email",
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "key": ""
}